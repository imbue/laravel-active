{
  "name": "tijdmachine/laravel-active",
  "description": "Helper to check if the current route is active.",
  "keywords": [
    "laravel",
    "route",
    "active"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Liam",
      "email": "hello@liamm.nl"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": ">=5.5"
  },
  "autoload": {
    "psr-4": {
      "Tijdmachine\\Active\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tijdmachine\\Active\\ActiveServiceProvider"
      ],
      "aliases": {
        "Active": "Tijdmachine\\Active\\Facade"
      }
    }
  }
}