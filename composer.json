{
    "name": "imbue/laravel-active",
    "description": "Helper to check if the current route is active.",
    "keywords": [
        "laravel",
        "route",
        "active"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
      {
        "name": "Liam",
        "email": "hello@liamm.nl"
      }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-4": {
          "Imbue\\Active\\": "src/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Imbue\\Active\\ActiveServiceProvider"
        ],
        "aliases": {
          "Active": "Imbue\\Active\\Facade"
        }
      }
    }
}